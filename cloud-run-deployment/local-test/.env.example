# Local test - No authentication (MCP server + ADK agent both run locally)
# Copy to .env and fill in your values:
#   cp .env.example .env

# Gemini API key (required for local - use Google AI Studio, not Vertex AI)
# Get key: https://aistudio.google.com/apikey
GOOGLE_API_KEY=your-gemini-api-key-here

# Disable Vertex AI - use API key instead
GOOGLE_GENAI_USE_VERTEXAI=FALSE

# MCP server URL - use /sse for SSE transport (default: local on port 9090)
MCP_SERVER_URL=http://localhost:9090/sse

# Optional
GOOGLE_MODEL=gemini-2.5-flash
